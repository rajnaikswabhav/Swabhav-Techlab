<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://gsmktg.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>gsmktg.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>gsmktg</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName>$gsmktg</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects>
        <ObjectGroup Name="EuphoriaContext" Order="1" Enabled="True" xmlns="">
          <Destination Path="Data Source=tcp:gsmktg.database.windows.net,1433;Initial Catalog=EuphoriaDB;User ID=kannan;Password=admin@123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="Techlabs.Euphoria.Kernel.Framework.Repository.EntityFramework.EuphoriaContext, Techlabs.Euphoria.Kernel" MigrationConfiguration="Techlabs.Euphoria.Kernel.Migrations.Configuration, Techlabs.Euphoria.Kernel" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)AuthContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:gsmktg.database.windows.net,1433;Initial Catalog=gsmktg_db;User Id=amdin@gsmktg;Password=admin@123</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)EuphoriaContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:gsmktg.database.windows.net,1433;Initial Catalog=EuphoriaDB;User ID=kannan;Password=admin@123</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="AuthContext" />
  </ItemGroup>
</Project>